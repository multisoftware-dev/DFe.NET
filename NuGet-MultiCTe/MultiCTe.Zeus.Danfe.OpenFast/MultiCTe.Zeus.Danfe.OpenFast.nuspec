<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>MultiCTe.Zeus.Danfe.OpenFast</id>
    <version>$version$</version>
    <title>Zeus NF-e e NFC-e DANFE OpenFastReport (x86/x64)</title>
    <authors>Zeus</authors>
    <owners>Zeus</owners>
    <projectUrl>https://github.com/ZeusAutomacao/DFe.NET</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Biblioteca para geração de DANFE usando FastReport.OpenSource.</description>
    <releaseNotes />
    <copyright>zeus</copyright>
    <language>pt-BR</language>
    <tags>nfc-e nf-e nfce nfe danfe fastreport</tags>
    <dependencies>
      <group targetFramework="netstandard2.0">
        <dependency id="MultiCTe.Zeus.NFe" version="$version$" />
        <dependency id="System.Drawing.Common" version="7.0.0" />
        <dependency id="FastReport.OpenSource" version="2022.2.2" />
        <dependency id="FastReport.OpenSource.Export.PdfSimple" version="2022.2.2" />
      </group>

      <group targetFramework="net8.0">
        <dependency id="MultiCTe.Zeus.NFe" version="$version$" />
        <dependency id="System.Drawing.Common" version="8.0.1" />
        <dependency id="FastReport.OpenSource" version="2022.2.2" />
        <dependency id="FastReport.OpenSource.Export.PdfSimple" version="2022.2.2" />
      </group>

      <group targetFramework=".NETFramework4.7.2">
        <dependency id="MultiCTe.Zeus.NFe" version="$version$" />
        <dependency id="FastReport.OpenSource" version="2022.2.2" />
        <dependency id="FastReport.OpenSource.Export.PdfSimple" version="2022.2.2" />
      </group>

      <group targetFramework=".NETFramework4.8">
        <dependency id="MultiCTe.Zeus.NFe" version="$version$" />
        <dependency id="FastReport.OpenSource" version="2022.2.2" />
        <dependency id="FastReport.OpenSource.Export.PdfSimple" version="2022.2.2" />
      </group>
    </dependencies>
    <contentFiles>
      <files include="any/any/NFe/*.frx" buildAction="Content" copyToOutput="true" flatten="false" />
      <files include="any/any/NFCe/*.frx" buildAction="Content" copyToOutput="true" flatten="false" />
    </contentFiles>
  </metadata>
  <files>

    <file src="..\..\NFe.Danfe.Base\NFe\*.frx" target="contentFiles\any\any\NFe" />
    <file src="..\..\NFe.Danfe.Base\NFCe\*.frx" target="contentFiles\any\any\NFCe" />

    <!-- netstandard2.0 -->
    <file src="..\..\NFe.Danfe.Base\bin\Release\netstandard2.0\NFe.Danfe.Base.dll"
      target="lib/netstandard2.0/NFe.Danfe.Base.dll" />
    <file src="..\..\NFe.Danfe.OpenFast\bin\Release\netstandard2.0\NFe.Danfe.OpenFast.dll"
      target="lib/netstandard2.0/NFe.Danfe.OpenFast.dll" />

    <!-- net8.0 -->
    <file src="..\..\NFe.Danfe.Base\bin\Release\net8.0\NFe.Danfe.Base.dll"
      target="lib/net8.0/NFe.Danfe.Base.dll" />
    <file src="..\..\NFe.Danfe.OpenFast\bin\Release\net8.0\NFe.Danfe.OpenFast.dll"
      target="lib/net8.0/NFe.Danfe.OpenFast.dll" />

    <!-- net472 -->
    <file src="..\..\NFe.Danfe.Base\bin\Release\net472\NFe.Danfe.Base.dll"
      target="lib/net472/NFe.Danfe.Base.dll" />
    <file src="..\..\NFe.Danfe.OpenFast\bin\Release\net472\NFe.Danfe.OpenFast.dll"
      target="lib/net472/NFe.Danfe.OpenFast.dll" />

    <!-- net481 -->
    <file src="..\..\NFe.Danfe.Base\bin\Release\net48\NFe.Danfe.Base.dll"
      target="lib/net48/NFe.Danfe.Base.dll" />
    <file src="..\..\NFe.Danfe.OpenFast\bin\Release\net48\NFe.Danfe.OpenFast.dll"
      target="lib/net48/NFe.Danfe.OpenFast.dll" />
  </files>
</package>
